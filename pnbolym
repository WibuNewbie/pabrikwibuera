function Baris()
    b = 0
    for _,tile in pairs(getTiles()) do
        if tile.bg ==  IdBackgroundForBaris or tile.fg == IdBackgroundForBaris then
          b = b+1
            if getBot().name:lower() == NameBot[b] then 
                tp(tile.x,tile.y)
                sleep(500)
            end  
        end
    end
end

function sendPackets(a,s)
    sendPacket(s,a) 
    
print("Script Maded By Wibu#9143")
end
function Trashing()
    for i,trasher in ipairs(IdItemToTrash) do
        if findItem(trasher) > 0 then
            sendPackets(2, "action|trash\n|itemID|"..trasher)
            sleep(1000)
            sendPackets(2, "action|dialog_return\ndialog_name|trash_item\nitemID|"..trasher.."|\ncount|"..findItem(trasher))
            sleep(1000)
        end
    end
end

function Take(Ids)
    for _, object in pairs(getObjects()) do
        if object.id == Ids  then
            sleep(100)
            tp(math.floor(object.x/32), math.floor(object.y/32))
            sleep(500)
            collect(RangeAutoCollectForTakeBlock)
            if findItem(Ids) > 0 then
                break
            end
        end
    end
end
function Warp(a,s)
    sendPackets(3,"action|join_request\nname|"..a.."|"..s.."\ninvitedWorld|0")
    sleep(DelayJoin)
end

function GoBackground(Id)
    for i,tile in ipairs(getTiles()) do
        if tile.bg == Id or tile.fg == Id then
            tp(tile.x+1,tile.y)
            sleep(500)
            break
        end
    end
end

function DropSomething(IdToDrop,IDBG)
    GoBackground(IDBG)
    sleep(500)
    while findItem(IdToDrop) > 0 do
        drop(IdToDrop)
        sleep(3000)
        move(1,0)
        sleep(1000)
    end
end

function DropPack()
    GoBackground(IdBackgroundForDropPack)
    sleep(500)
    for i = 1,20 do
        for is = 1,#IdPack do
            while findItem(IdPack[is]) > 0 do
                drop(IdPack[is])
                sleep(3000)
                 move(1,0)
                sleep(1000)
            end
        end
    end
end

function hitTile(px,py)
punch(px,py)
pkt = {}
pkt.type = 0
pkt.flags = 3
pkt.pos_x = getBot().x
pkt.pos_y = getBot().y
pkt.int_x = math.floor(getBot().x / 32) + px
pkt.int_y = math.floor(getBot().y / 32) + py
sendPacketRaw(pkt)
end

function placeTile(pid,px,py)
place(pid,px,py)
pkt = {}
pkt.type = 0
pkt.flags = 3
pkt.int_data = pid
pkt.pos_x = getBot().x
pkt.pos_y = getBot().y
pkt.int_x = math.floor(getBot().x / 32) + px
pkt.int_y = math.floor(getBot().y / 32) + py
sendPacketRaw(pkt)
end

function gg()
for i, tile in ipairs(getTiles()) do
if tile.fg == 6946 then
findPath(tile.x,tile.y+1,100)
sleep(280)
break
end
end
end

function ds()
for i, tile in ipairs(getTiles()) do
if  tile.bg == IdBackgroundForDropSeed  then
findPath(tile.x-1,tile.y,100)
sleep(1500)
drop(IdBlock+1)
sleep(1300)
if findItem(IdBlock+1) == 0 then
break
end
end
end
end

function db()
for i, tile in ipairs(getTiles()) do
if tile.bg == IdBgForDropBlock   then
findPath(tile.x-1,tile.y,100)
sleep(1500)
drop(IdBlock)
sleep(1300)
if findItem(IdBlock) == 0 then
break
end
end
end
end

function gu()
for i, tile in ipairs(getTiles()) do
if tile.fg == 6948 then
findPath(tile.x,tile.y+1,100)
sleep(280)
break
end
end
end
function rets(v)
    if v[0] == "OnDialogRequest" then
        count = v[1]:match("|Amount:|(%d+)") 
        x = v[1]:match("|tilex|(%d+)")
        y = v[1]:match("|tiley|(%d+)")
        if v[1]:find("end_dialog|itemsucker_seed|Close|Update") and v[1]:find("retrieveitem") then
sendPackets(2,[[action|dialog_return
dialog_name|itemsucker_seed
tilex|]]..x..[[|
tiley|]]..y..[[|
buttonClicked|retrieveitem
chk_enablesucking|1]])
elseif v[1]:find("end_dialog|itemremovedfromsucker|Close|Retrieve") then
sendPackets(2,[[action|dialog_return
dialog_name|itemremovedfromsucker
tilex|]]..x..[[|
tiley|]]..y..[[|
itemtoremove|]]..count)

elseif v[1]:find("end_dialog|itemsucker_block|Close|Update") and v[1]:find("retrieveitem") then
sendPackets(2,[[action|dialog_return
dialog_name|itemsucker_block
tilex|]]..x..[[|
tiley|]]..y..[[|
buttonClicked|retrieveitem
chk_enablesucking|1]])
print("Script Maded By Wibu#9143")
          return true
        end

    end
end
